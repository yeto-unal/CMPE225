#include<iostream>
#include<string>
using namespace std;
class deneme{
	private:
		int x;
		int c=20;
		string a;
	public:
		int b=10;
		void get(){
			cout<<"Enter a number: "<<endl;
			cin>>x;
			cout<<"Enter a string: "<<endl;
			cin>>a;
		}
		void show(){
			cout<<"Number is: "<<x<<endl;
			cout<<"String is: "<<a<<endl;
		}
		void show2(){
			cout<<c;
		}	
};

main(){
	deneme Y;
	Y.get();
	Y.show();
	cout<<"Public number is: "<<Y.b<<endl;
	cout<<"Private number is: ";
	Y.show2();
	return 0;
}
