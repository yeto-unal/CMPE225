#include<iostream>
using namespace std;

class deneme{
	private:
		int data1;
		float data2;
	public:
		void getInt (int d){
			data1=d;
			cout<<"Integer data is "<<data1<<endl;
		}
		float getFlt(){
			cout<<"Enter float: ";
			cin>>data2;
			return data2;
		}	
};

main(){
	deneme o1;
	float fltdata;
	o1.getInt(12);
	fltdata=o1.getFlt();
	//cout<<"Integer data is "<<o1.data1;
	//cout<<"Float data is "<<o1.data2;
	cout<<"Float data is "<<fltdata;
}
