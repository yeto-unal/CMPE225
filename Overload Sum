#include<iostream>
using namespace std;

int sum(int a, int b){
	return a+b;
}

int sum(int a, int b,int c){
	return a+b+c;
}

double sum(double a, double b){
	return a+b;
}

main(){
	int x,y,a=10,b=20,c=-5;
	double n,m;
	cout<<"Enter 2 integers: "<<endl;
	cin>>x;
	cin>>y;
	cout<<"Sum is: " <<sum(x,y)<<endl;
	cout<<"Enter 2 doubles:\n ";
	cin>>n;
	cin>>m;
	cout<<"Sum is: "<<sum(n,m)<<endl;
	cout<<"Constant sum is: "<<sum(a,b,c);
	return 0;
}
